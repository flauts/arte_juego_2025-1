{
  "#popup_text": {
    "colours": {
      "normal_border": "transparent"
    }
  },
  "#accept_button": {
    "colours": {
      "normal_bg": "green"
    }
  },
    "@canvas_icon": {
        "images": {
            "normal_image": {
                "path": "ui_icons/canvas.png"
            }
        }
    },
    "@chat_icon": {
        "images": {
            "normal_image": {
                "path": "ui_icons/chat.png"
            }
        }
    },
    "#icon": {
        "font": {
            "name": "retro",
            "size": 16,
            "regular_path": "fonts/retrofont.ttf"
        },
        "colours": {
            "normal_bg": "#cdc0b0",
            "normal_border": "#cdc0b0",
            "hovered_bg": "#cdc0b0",
            "active_bg": "#cdc0b0"

        }
    },
    "#message_window":{
     "font": {
            "name": "retro",
            "size": 16,
            "regular_path": "fonts/retrofont.ttf"
        },
        "colours": {
        "normal_bg": "#222222",
        "normal_border": "#222222",
        "hovered_bg": "#222222",
        "active_border": "#222222",
        "normal_text": "#FFAA00",
        "title_bg": "#222222",
        "title_text": "#FFAA00"
    },
    "misc": {
        "shape_corner_radius": 0,
        "border_width": {
            "left": 0,
            "right": 0,
            "top": 0,
            "bottom": 0
        },
           "shadow_width": 4,
   "shadow_color": "#000000"
    }

    },
    "defaults": {
        "colours":
         {
             "normal_bg":"#45494e",
             "hovered_bg":"#35393e",
             "disabled_bg":"#25292e",
             "selected_bg":"#193754",
             "dark_bg":"#15191e",
             "normal_text":"#c5cbd8",
             "hovered_text":"#FFFFFF",
             "selected_text":"#FFFFFF",
             "disabled_text":"#6d736f",
             "link_text": "#0000EE",
             "link_hover": "#2020FF",
             "link_selected": "#551A8B",
             "text_shadow": "#777777",
             "normal_border": "#DDDDDD",
             "hovered_border": "#B0B0B0",
             "disabled_border": "#808080",
             "selected_border": "#8080B0",
             "active_border": "#8080B0",
             "filled_bar":"#f4251b",
             "unfilled_bar":"#CCCCCC"
         },
        "misc": {
            "shape": "rectangle",
            "border_width": {
                "left": 1,
                "right": 1,
                "top": 1,
                "bottom": 1
            },
            "shadow_width": 2,
            "shape_corner_radius": 2,
            "border_overlap": 1,
            "enable_title_bar": "1",
            "text_horiz_alignment": "left",
            "text_vert_alignment": "center",
            "text_horiz_alignment_padding": 4,
            "text_vert_alignment_padding": 4,
            "text_shadow_size": 1,
            "text_shadow_offset": [
                1,
                1
            ],
            "tool_tip_delay": 1.0,
            "title_bar_height": 28
        }
    }
}